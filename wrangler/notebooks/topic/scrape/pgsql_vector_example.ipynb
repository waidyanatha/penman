   "source": [
    "## # Database connection details"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "973c6cd5",
   "metadata": {},
   "outputs": [],
   "source": [
    "db_properties = {\n",
    "    \"url\": \"jdbc:postgresql://localhost:5432/penman\",\n",
