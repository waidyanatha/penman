   "cell_type": "markdown",
   "id": "0a4f70be",
   "metadata": {},
   "source": [
    "# Embeding vector store in Postgresql"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "id": "0060e1ed",
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "from pyspark.sql import SparkSession\n",
    "from pyspark.sql.functions import udf, col, concat, lit\n",
    "from pyspark.sql.types import ArrayType, FloatType, IntegerType, StringType\n",
    "from pyspark.sql.types import StructType, StructField, DataType\n",
    "from sentence_transformers import SentenceTransformer\n",
    "from duckduckgo_search import DDGS\n",
    "from pgvector.sqlalchemy import Vector\n",
    "from sqlalchemy import create_engine, Column, Integer, String\n",
    "from sqlalchemy.orm import declarative_base\n",
    "from sqlalchemy.dialects.postgresql import ARRAY\n"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "cf3b1445",
   "metadata": {},
   "source": [
    "## # Initialize Spark Session"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "608bb2cd",
   "metadata": {},
   "outputs": [
